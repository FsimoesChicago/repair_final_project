<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Service provider #<%= @service_provider.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/service_providers" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link service_provider start -->
      <div class="col">
        <a href="/service_providers/<%= @service_provider.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit service provider
        </a>
      </div>
      <!-- Edit link service_provider end -->

    </div>

    <dl>
      <dt>
        Email
      </dt>
      <!-- Display email start -->
      <dd>
        <%= @service_provider.email %>
      </dd>
      <!-- Display email end -->

      <dt>
        Password
      </dt>
      <!-- Display password start -->
      <dd>
        <%= @service_provider.password %>
      </dd>
      <!-- Display password end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@service_provider.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@service_provider.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->

<!-- Show Page Customization end -->
