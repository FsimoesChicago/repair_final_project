<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Service provider #<%= @service_provider.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/service_providers" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/service_providers/<%= @service_provider.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit service provider
        </a>
      </div>

    </div>

    <dl>
      <dt>
        Email
      </dt>
      <dd>
        <%= @service_provider.email %>
      </dd>

      <dt>
        Password
      </dt>
      <dd>
        <%= @service_provider.password %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@service_provider.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@service_provider.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "service_provider_templates/service_provider_services" %>
  </div>
</div>
